<Window x:Class="HaCreator.GUI.ObjectViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:editorItems="clr-namespace:HaCreator.GUI.EditorPanels"
        Title="Object Viewer"
        Height="500"
        Width="320"
        MinHeight="300"
        MinWidth="250"
        WindowStyle="ToolWindow"
        ShowInTaskbar="False"
        Topmost="False">
    <Grid>
        <WindowsFormsHost x:Name="objectViewerPanelHost">
            <editorItems:ObjectViewerPanel x:Name="objectViewerPanel"/>
        </WindowsFormsHost>
    </Grid>
</Window>
